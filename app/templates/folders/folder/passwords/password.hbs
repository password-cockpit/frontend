<h1>
    <i class="fas fa-{{model.password.icon}}"></i> 
    {{model.password.title}}
</h1>
{{password/password-content 
    password=model.password 
    folder=model.folder 
    onDeletePassword=(action "onDeletePassword") 
    onSavePassword=(action "onSavePassword")
    canAccessAllFolders=canAccessAllFolders
    isPasswordVisible=model.isPasswordVisible
    isEdit=model.isEdit
    errors=updatingPasswordErrors
    canViewLogs=canViewLogs
}}
{{#if canViewLogs}}
    {{password/password-log 
        logs=logs 
        page=page
        pageCount=pageCount
        changeLogPage=(action "changeLogPage")
        refreshLog=(action "refreshLog")
        password=model.password
        users=model.users
    }}
{{/if}}
{{outlet}}